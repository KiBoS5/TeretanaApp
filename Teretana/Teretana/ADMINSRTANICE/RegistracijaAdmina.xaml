        <Button Content="REGISTRUJ" HorizontalAlignment="Left" Height="32" Margin="175,342,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click_1" Background="#FFA7A7A7" BorderBrush="Black"/>
